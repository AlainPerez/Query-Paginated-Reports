      <DataSourceReference>ShDS_AdventureWorks</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>04f75d1d-927f-4815-b0e1-46714e9a4895</rd:DataSourceID>
